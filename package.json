{
  "name": "colour-swapper",
  "version": "1.0.3",
  "description": "colour-swapper is a compact and efficient JavaScript library designed for color manipulation and conversion",
  "author": "Umanda Jayobandara",
  "license": "MIT",
  "homepage": "https://github.com/umanda/colour-swapper",
  "repository": {
    "type": "git",
    "url": "https://github.com/umanda/colour-swapper"
  },
  "bugs": {
    "url": "https://github.com/umanda/colour-swapper/issues"
  },
  "keywords": [
    "grayscale",
    "rgb",
    "tinycolor2",
    "colour",
    "swap",
    "utility"
  ],
  "type": "module",
  "main": "./dist/colour-swapper.umd.cjs",
  "module": "./dist/colour-swapper.js",
  "types": "./dist/colour-swapper.d.ts",
  "typings": "./typings/colour-swapper.d.ts",
  "exports": {
    ".": {
      "import": "./dist/colour-swapper.js",
      "require": "./dist/colour-swapper.umd.cjs",
      "types": "./typings/colour-swapper.d.ts"
    }
  },
  "files": [
    "dist",
    "typings"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/tinycolor2": "^1.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.1",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.8.25"
  },
  "dependencies": {
    "tinycolor2": "^1.6.0"
  }
}
